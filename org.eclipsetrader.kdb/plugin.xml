<?xml version="1.0" encoding="UTF-8"?>
<?eclipse version="3.2"?>
<plugin>
   <extension
         name="K-DB"
         point="org.eclipsetrader.core.connectors">
      <connector
            class="org.eclipsetrader.kdb.internal.core.connector.FeedConnector"
            id="org.eclipsetrader.kdb"
            name="K-DB">
      </connector>
      <backfill
            class="org.eclipsetrader.kdb.internal.core.connector.BackfillConnector"
            id="org.eclipsetrader.kdb"
            name="K-DB">
      </backfill>
      <property
            id="org.eclipsetrader.kdb.symbol"
            name="Code">
      </property>
   </extension>
   <extension
         point="org.eclipse.ui.preferencePages">
      <page
            category="org.eclipsetrader.ui.preferences.plugins"
            class="org.eclipsetrader.kdb.internal.ui.preferences.AuthenticationPage"
            id="org.eclipsetrader.kdb.general"
            name="K-DB">
      </page>
   </extension>
   <extension
         point="org.eclipse.ui.importWizards">
      <wizard
            category="org.eclipsetrader.ui.import.data"
            class="org.eclipsetrader.kdb.internal.ui.wizards.DataImportWizard"
            id="org.eclipsetrader.kdb.data"
            name="K-DB">
         <description>
            Import historical data from K-DB.
         </description>
      </wizard>
   </extension>
   <extension
         point="org.eclipsetrader.core.providers">
      <provider
            class="org.eclipsetrader.kdb.internal.ui.FeedIdentifierFactory"
            id="org.eclipsetrader.kdb.providers.symbol"
            name="Code">
      </provider>
   </extension>

</plugin>
